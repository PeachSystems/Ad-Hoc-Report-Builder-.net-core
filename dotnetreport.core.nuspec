        <copyright>(c) dotnet Report Builder</copyright>
        <tags>embedded analytics, report generator, .net core report builder, sql reporting tools, visual studio report designer, ad-hoc reporting, asp .net mvc report, report builder, ad hoc adhoc reports, viewer, reportviewer, sql builder mvc, web forms, query builder,mvc reporting,custom query,query maker, dotnetreport</tags>
        <dependencies>            
            <dependency id="EPPlus" version="4.1.0" />
            <dependency id="Newtonsoft.Json" version="9.0.1" />            
            <dependency id="Quartz" version="3.0.7" />
            <dependency id="System.Net.Http" version="4.3.4"></dependency>
        </dependencies>
      <contentFiles>
